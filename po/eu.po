#: ../data/com.ubuntu.developer.rschroll.gmail_gmail.ini.in.h:1
msgid "Unofficial Gmail scope"
msgstr "Gmail-en eremu ez-ofiziala"

#: ../data/com.ubuntu.developer.rschroll.gmail_gmail-settings.ini.in.h:1
msgid "Name"
msgstr "Izena"

#: ../data/com.ubuntu.developer.rschroll.gmail_gmail-settings.ini.in.h:2
msgid "View messages..."
msgstr "Ikusi mezuak..."

#. / Three options for viewing messages.  Translate each individually and join with semicolons.
#: ../data/com.ubuntu.developer.rschroll.gmail_gmail-settings.ini.in.h:4
msgid "In threads;Individually;Individually with snippets"
msgstr "Harituta;Banaka;Banaka mezu zatiekin"

#. / These strings give short forms of dates and times, depending on how recent they are.
#. / Use the codes from http://qt-project.org/doc/qt-5/qdatetime.html#fromString-2
#. / In the past day
#: ../src/scope/query.cpp:132
msgid "HH:mm"
msgstr "HH:mm"

#. / In the past week
#: ../src/scope/query.cpp:135
msgid "dddd"
msgstr "dddd"

#. / In the past year
#: ../src/scope/query.cpp:138
msgid "MMM d"
msgstr "MMM d"

#. / More than a year ago
#: ../src/scope/query.cpp:140
msgid "MMM d, yyyy"
msgstr "yyyy MMM d"

#: ../src/scope/query.cpp:200
msgid "Inbox"
msgstr "Sarrera-ontzia"

#: ../src/scope/query.cpp:207
msgid "All mail"
msgstr "Mezu guztiak"

#: ../src/scope/query.cpp:300
msgid "From:"
msgstr "Nork:"

#: ../src/scope/query.cpp:303
msgid "To:"
msgstr "Nori:"

#: ../src/scope/query.cpp:306
msgid "Cc:"
msgstr "Kopia:"

#: ../src/scope/query.cpp:320
msgid "More results..."
msgstr "Emaitza gehiago..."

#: ../src/scope/query.cpp:332
msgid "Log in with Google"
msgstr "Hasi saioa Googlerekin"

#. / Header above buttons with following two strings
#: ../src/scope/preview.cpp:51
msgid "Find messages"
msgstr "Bilatu mezuak"

#. / Argument is name or email address
#: ../src/scope/preview.cpp:60
#, c-format
msgid "From %s"
msgstr "Nork: %s"

#: ../src/scope/preview.cpp:64
msgid "In thread"
msgstr "Harituta"

#. / Argument is name or email address
#: ../src/scope/preview.cpp:76
#, c-format
msgid "Reply to %s"
msgstr "Erantzun %s(e)ri"

#: ../src/scope/preview.cpp:82
msgid "View in Gmail"
msgstr "Ikusi Gmail-en"

#: ../src/scope/preview.cpp:107
msgid "Mark as read"
msgstr "Markatu irakurritako gisa"

#: ../src/scope/preview.cpp:108
msgid "Mark as unread"
msgstr "Markatu irakurri gabeko gisa"

#: ../src/scope/preview.cpp:110
msgid "Remove from trash"
msgstr "Kendu zakarrontzitik"

#: ../src/scope/preview.cpp:111
msgid "Move to trash"
msgstr "Bota zakarrontzira"
