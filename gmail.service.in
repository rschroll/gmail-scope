<?xml version="1.0" encoding="UTF-8"?>
<service id="@SCOPE_INSTALL_NAME@">
  <type>@SCOPE_INSTALL_NAME@</type>
  <name>Gmail</name>
  <provider>google</provider>
  <translations>gmail-scope</translations>
  <template>
    <group name="auth">
      <setting name="method">oauth2</setting>
      <setting name="mechanism">web_server</setting>
      <group name="oauth2">
        <group name="web_server">
          <setting type="as" name="Scope">['https://www.googleapis.com/auth/gmail.readonly']</setting>
          <setting name="ClientId">@CLIENT_ID@</setting>
          <setting name="ClientSecret">@CLIENT_SECRET@</setting>
        </group>
      </group>
    </group>
  </template>
</service>
